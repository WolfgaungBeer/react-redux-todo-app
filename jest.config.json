{
  "bail": true,
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom" : [
      "**/src/**/*.js",
      "!**/node_modules/**",
      "!**/*.story.js",
      "!**/selectors.js",
      "!src/index.js",
      "!src/store.js",
      "!src/theme.js",
      "!**/scenes/**"
  ],
  "coverageDirectory": "./__coverage__",
  "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
}
